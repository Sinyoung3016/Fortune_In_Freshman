<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab09_P.input_controller">
		<children>
			<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="30.0">
				<children>
					<Label text="ID" />
					<TextField fx:id="titleField" HBox.hgrow="ALWAYS">
						<HBox.margin>
							<Insets />
						</HBox.margin>
					</TextField>
				</children>
				<padding>
					<Insets left="30.0" right="30.0" />
				</padding>
			</HBox>
			<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
				<children>
					<Label text="Password" />
					<TextField fx:id="passwordField" HBox.hgrow="ALWAYS" />
				</children>
				<padding>
					<Insets left="30.0" right="30.0" />
				</padding>
			</HBox>
			<HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
				<children>
					<Label text="공개" />
					<ChoiceBox fx:id="publicBox" prefWidth="150.0" />
					<Label text="게시 종료" />
					<DatePicker fx:id="endDatePicker" />
				</children>
				<padding>
					<Insets left="30.0" right="30.0" />
				</padding>
			</HBox>
			<HBox prefHeight="200.0" prefWidth="600.0">
				<children>
					<TextArea fx:id="contentsArea" prefHeight="200.0" prefWidth="600.0" />
				</children>
				<padding>
					<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				</padding>
			</HBox>
			<HBox alignment="CENTER" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
				<children>
					<Button mnemonicParsing="false" onAction="#registerAction" text="확인" />
					<Button mnemonicParsing="false" onAction="#cancelAction" text="취소" />
				</children>
			</HBox>
		</children>
	</VBox>
